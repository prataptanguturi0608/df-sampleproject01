{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "df-sampleproject01"
		},
		"ls_Activitylog_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'ls_Activitylog'"
		},
		"ls_AzBlob_Archive_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'ls_AzBlob_Archive'"
		},
		"ls_AzBlob_landing_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'ls_AzBlob_landing'"
		},
		"ls_AzDlake_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'ls_AzDlake'"
		},
		"ls_AzureSqlDatabase_password": {
			"type": "secureString",
			"metadata": "Secure string for 'password' of 'ls_AzureSqlDatabase'"
		},
		"ls_Sftp_source_password": {
			"type": "secureString",
			"metadata": "Secure string for 'password' of 'ls_Sftp_source'"
		},
		"ls_AzDlake_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://dlakeadf.dfs.core.windows.net/"
		},
		"ls_AzureSqlDatabase_properties_typeProperties_server": {
			"type": "string",
			"defaultValue": "mfsdbserv01.database.windows.net"
		},
		"ls_AzureSqlDatabase_properties_typeProperties_database": {
			"type": "string",
			"defaultValue": "SampleProject01"
		},
		"ls_AzureSqlDatabase_properties_typeProperties_userName": {
			"type": "string",
			"defaultValue": "sa-airbus"
		},
		"ls_Sftp_source_properties_typeProperties_host": {
			"type": "string",
			"defaultValue": "eu-central-1.sftpcloud.io"
		},
		"ls_Sftp_source_properties_typeProperties_userName": {
			"type": "string",
			"defaultValue": "819a9d6fe5aa44c6b2b473c9ef36a63d"
		},
		"ls_ADB_BronzeToSilver_properties_typeProperties_existingClusterId": {
			"type": "string",
			"defaultValue": "masternode"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "df-sampleproject01_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"ls_Activitylog_connectionString": {
						"value": "[parameters('ls_Activitylog_connectionString')]"
					},
					"ls_AzBlob_Archive_connectionString": {
						"value": "[parameters('ls_AzBlob_Archive_connectionString')]"
					},
					"ls_AzBlob_landing_connectionString": {
						"value": "[parameters('ls_AzBlob_landing_connectionString')]"
					},
					"ls_AzDlake_accountKey": {
						"value": "[parameters('ls_AzDlake_accountKey')]"
					},
					"ls_AzureSqlDatabase_password": {
						"value": "[parameters('ls_AzureSqlDatabase_password')]"
					},
					"ls_Sftp_source_password": {
						"value": "[parameters('ls_Sftp_source_password')]"
					},
					"ls_AzDlake_properties_typeProperties_url": {
						"value": "[parameters('ls_AzDlake_properties_typeProperties_url')]"
					},
					"ls_AzureSqlDatabase_properties_typeProperties_server": {
						"value": "[parameters('ls_AzureSqlDatabase_properties_typeProperties_server')]"
					},
					"ls_AzureSqlDatabase_properties_typeProperties_database": {
						"value": "[parameters('ls_AzureSqlDatabase_properties_typeProperties_database')]"
					},
					"ls_AzureSqlDatabase_properties_typeProperties_userName": {
						"value": "[parameters('ls_AzureSqlDatabase_properties_typeProperties_userName')]"
					},
					"ls_Sftp_source_properties_typeProperties_host": {
						"value": "[parameters('ls_Sftp_source_properties_typeProperties_host')]"
					},
					"ls_Sftp_source_properties_typeProperties_userName": {
						"value": "[parameters('ls_Sftp_source_properties_typeProperties_userName')]"
					},
					"ls_ADB_BronzeToSilver_properties_typeProperties_existingClusterId": {
						"value": "[parameters('ls_ADB_BronzeToSilver_properties_typeProperties_existingClusterId')]"
					}
				}
			},
			"dependsOn": []
		},
		{
			"name": "df-sampleproject01_ArmTemplate_1",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_1.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					}
				}
			},
			"dependsOn": [
				"[resourceId('Microsoft.Resources/deployments', 'df-sampleproject01_ArmTemplate_0')]"
			]
		}
	]
}