				"fileName": {
					"value": "@dataset().p_fileName",
					"type": "Expression"
				},
				"folderPath": {
					"value": "@formatDateTime(utcNow(),'yyyy/MM/dd')",
					"type": "Expression"
				},
				"container": "archive"
			}
		}
	}
}